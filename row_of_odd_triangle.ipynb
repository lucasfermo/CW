   "source": [
    "def odd_row(n):\n",
    "    c=1\n",
    "    start=1\n",
    "    while c<n:\n",
    "        start+=2*c\n",
    "        c+=1\n",
    "    r=[start+i*2 for i in range(n)]\n",
    "    return r\n",
    "\n",
    "odd_row(3)"
   ]
  }
