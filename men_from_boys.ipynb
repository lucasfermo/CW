    "def men_from_boys(arr):\n",
    "    return sum([sorted(list(set([x for x in arr if not x%2]))),\n",
    "                         sorted(list(set([x for x in arr if x%2])))[::-1]],[])\n",
    "\n",
    "men_from_boys([2,15,17,15,2,10,10,17,1,1])"
   ]
