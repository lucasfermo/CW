    "def solve(s):\n",
    "    temp,r=0,0\n",
    "    for x in s:\n",
    "        temp+=ord(x)-96\n",
    "        if x in 'aeiou':\n",
    "            temp=0\n",
    "        if temp>r:\n",
    "            r=temp\n",
    "    return r\n",
    "\n",
    "solve('zcod')"
   ]
