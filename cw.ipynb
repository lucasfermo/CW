    "def high(x):\n",
    "    sums = [sum([ord(x)-96 for x in y]) for y in x.split(\" \")]\n",
    "    return  x.split(\" \")[sums.index(max(sums))]\n",
    "\n",
    "high('what time are we climbing up the volcano')"
   ]
