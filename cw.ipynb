    "def parts_sums(ls):\n",
    "    s=sum(ls)\n",
    "    r=[]\n",
    "    for v in ls:\n",
    "        r.append(s)\n",
    "        s-=v\n",
    "        \n",
    "    return r + [0]\n",
    "\n",
    "\n",
    "\n",
    "ls = [0, 1, 3, 6, 10]\n",
    "parts_sums(ls)"
   ]
  }
