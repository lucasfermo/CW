    "def solution(s):\n",
    "    r=[]\n",
    "    for c in s:\n",
    "        if ord(c) >= 65 and ord(c) <=90:\n",
    "            r.append(\" \" + c)\n",
    "        else:\n",
    "            r.append(c)\n",
    "    return \"\".join(r)\n",
    "    \n",
    "solution(\"lucasFermo\")"
   ]
  }
