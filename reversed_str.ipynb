    "def add_letters(*letters):\n",
    "    if not len(letters):\n",
    "        return \"z\"\n",
    "    r=0\n",
    "    for v in letters:\n",
    "        r+=(ord(v)-96)\n",
    "    r=r%26\n",
    "    if not r: r=26\n",
    "    return chr(96 + r%26)"
   ]
