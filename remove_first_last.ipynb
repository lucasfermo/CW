    "def range_bit_count(a, b):\n",
    "    return sum(sum([[int(x) for x in to_bin(v)] for v in range(a,b+1)],[]))\n",
    "    \n",
    "def to_bin(n):\n",
    "    r=\"\"\n",
    "    while n>0:\n",
    "        r+=str(n%2)\n",
    "        n//=2\n",
    "    return r[::-1]\n",
    "\n",
    "range_bit_count(2,7)"
   ]
  }
