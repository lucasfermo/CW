    "#x2 - 4 * y2 = (x - 2*y) * (x + 2*y)\n",
    "\n",
    "\n",
    "def sol_equa(n):\n",
    "    m=math.ceil(math.sqrt(n))\n",
    "    for i in range(m,n):\n",
    "        x=i**2\n",
    "        f=(x-n)/4\n",
    "        y=math.sqrt(f)\n",
    "        if y==int(y):\n",
    "            return [[i,int(y)]]\n",
    "        \n",
    "       \n",
    "\n",
    "sol_equa(1116753)"
   ]
