    "def exchange_with(a, b):\n",
    "    a1,b1=a.copy(),b.copy()\n",
    "    \n",
    "    for x in a1: a.pop()\n",
    "    for x in b1[::-1]: a.append(x)\n",
    "    for x in b1: b.pop()\n",
    "    for x in a1[::-1]: b.append(x)\n",
    "        \n",
    "    #a[i] = x for x in b[::-1]\n",
    "\n",
    "a=[1,2,3]\n",
    "b=list(\"abc\")\n",
    "\n",
    "exchange_with(a,b)\n",
    "b"
   ]
